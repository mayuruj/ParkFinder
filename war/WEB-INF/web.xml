<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ParkFinder</servlet-name>
		<servlet-class>com.appwizards.parkfinder.ParkFinderServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SignUpServlet</servlet-name>
		<servlet-class>com.appwizards.parkfinder.SignUpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ParkFinder</servlet-name>
		<url-pattern>/parkfinder</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LoginValidate</servlet-name>
		<servlet-class>com.appwizards.parkfinder.LoginValidate</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginValidate</servlet-name>
		<url-pattern>/loginvalidate</url-pattern>
		
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>SignUpServlet</servlet-name>
		<url-pattern>/signup</url-pattern>
		
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
